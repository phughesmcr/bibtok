{
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "css.customData": [
    ".vscode/tailwind.json"
  ],
  "editor.rulers": [
    120
  ],
  "cSpell.words": [
    "BBCCCVVV",
    "BCCCVVV",
    "callout",
    "crossrefs",
    "msapplication",
    "phugh",
    "preact",
    "shortlink",
    "tailwindcss",
    "tiktok"
  ],
  "editor.formatOnSave": true
}
